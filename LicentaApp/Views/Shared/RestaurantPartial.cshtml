@model LicentaApp.Models.Restaurant


<div class="card-body">

    <h3 class="card-title alert-success py-3 px-3 rounded-2">@Model.Name</h3>

    <div class="card-text">@Html.Raw(Model.Name)</div>

    <div class="d-flex justify-content-between flex-row mt-5">

        <div><i class="bi bi-globe"></i> @Model.City</div>

        <div><i class="bi bi-person"></i> @Model.Name</div>

    </div>

</div>


